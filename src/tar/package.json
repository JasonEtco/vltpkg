{
  "name": "@vltpkg/tar",
  "description": "An extremely limited and very fast tar extractor",
  "version": "0.0.0-0",
  "tshy": {
    "dialects": [
      "esm"
    ],
    "exports": {
      "./package.json": "./package.json",
      ".": "./src/index.ts",
      "./unpack": "./src/unpack.ts",
      "./unpack-request": "./src/unpack-request.ts",
      "./pool": "./src/pool.ts"
    },
    "selfLink": false
  },
  "dependencies": {
    "@vltpkg/error-cause": "workspace:*",
    "rimraf": "^5.0.5",
    "tar": "^7.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@types/tar": "^6.1.13",
    "mutate-fs": "^2.1.1",
    "pacote": "^18.0.6",
    "prettier": "^3.2.5",
    "tap": "^19.2.5",
    "tshy": "^1.15.1"
  },
  "scripts": {
    "preversion": "npm test",
    "postversion": "npm publish",
    "prepublishOnly": "git push origin --follow-tags",
    "prepare": "tshy",
    "pretest": "npm run prepare",
    "presnap": "npm run prepare",
    "test": "tap",
    "snap": "tap",
    "format": "prettier --write . --log-level warn --ignore-path ../../.prettierignore --cache",
    "typedoc": "typedoc"
  },
  "tap": {
    "extends": "../../tap-config.yaml"
  },
  "prettier": "../../.prettierrc.json",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "source": "./src/index.ts",
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./unpack": {
      "import": {
        "source": "./src/unpack.ts",
        "types": "./dist/esm/unpack.d.ts",
        "default": "./dist/esm/unpack.js"
      }
    },
    "./unpack-request": {
      "import": {
        "source": "./src/unpack-request.ts",
        "types": "./dist/esm/unpack-request.d.ts",
        "default": "./dist/esm/unpack-request.js"
      }
    },
    "./pool": {
      "import": {
        "source": "./src/pool.ts",
        "types": "./dist/esm/pool.d.ts",
        "default": "./dist/esm/pool.js"
      }
    }
  }
}
