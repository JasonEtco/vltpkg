{
  "name": "@vltpkg/tar",
  "description": "An extremely limited and very fast tar extractor",
  "version": "0.0.0-0",
  "tap": {
    "coverage-map": "./map.js"
  },
  "scripts": {
    "preversion": "npm test",
    "postversion": "npm publish",
    "prepublishOnly": "git push origin --follow-tags",
    "prepare": "tshy",
    "pretest": "npm run prepare",
    "presnap": "npm run prepare",
    "test": "tap",
    "snap": "tap",
    "format": "prettier --write . --loglevel warn --ignore-path ../../.prettierignore --cache",
    "typedoc": "typedoc"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/tar": "^6.1.11",
    "tap": "^18.7.2",
    "tshy": "^1.14.0"
  },
  "dependencies": {
    "rimraf": "^5.0.5",
    "tar": "^7.0.1"
  },
  "tshy": {
    "dialects": [
      "esm"
    ],
    "exports": {
      "./package.json": "./package.json",
      ".": "./src/index.ts",
      "./unpack": "./src/unpack.ts",
      "./unpack-request": "./src/unpack-request.ts",
      "./pool": "./src/pool.ts"
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./unpack": {
      "import": {
        "types": "./dist/esm/unpack.d.ts",
        "default": "./dist/esm/unpack.js"
      }
    },
    "./unpack-request": {
      "import": {
        "types": "./dist/esm/unpack-request.d.ts",
        "default": "./dist/esm/unpack-request.js"
      }
    },
    "./pool": {
      "import": {
        "types": "./dist/esm/pool.d.ts",
        "default": "./dist/esm/pool.js"
      }
    }
  },
  "type": "module",
  "prettier": {
    "semi": false,
    "printWidth": 70,
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "bracketSameLine": true,
    "arrowParens": "avoid",
    "endOfLine": "lf"
  }
}
